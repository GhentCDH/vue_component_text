{
  "name": "vue_component_text",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "docs:dev": "vue-demi-switch 3 && vitepress dev docs",
    "docs:build": "vue-demi-switch 3 && vitepress build docs",
    "docs:preview": "vue-demi-switch 3 && vitepress preview docs",
    "test:vue2": "vue-demi-switch 2 && vitest --environment jsdom --root src/",
    "test:vue3": "vue-demi-switch 3 && vitest --environment jsdom --root src/",
    "test:coverage": "vue-demi-switch 3 && vitest --environment jsdom --root src/ --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "vue-demi": "^0.13.11"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vitest/coverage-c8": "^0.26.2",
    "@vue/composition-api": "^1.7.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "bootstrap": "^5.2.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "stylelint": "^14.16.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitepress": "^1.0.0-alpha.33",
    "vitest": "^0.25.6",
    "vue": "npm:vue@3.2.45",
    "vue-tsc": "^1.0.12",
    "vue2": "npm:vue@2.7.14"
  }
}
